{
    "name": "oriceon/oauth-5-laravel",
    "type": "library",
    "description": "OAuth Service Provider for Laravel 5",
    "keywords": ["OAuth", "Lusitanian", "laravel", "php"],
    "homepage": "https://github.com/oriceon/oauth-5-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Artdarek",
            "email": "artdarek@gmail.com",
            "role": "Developer"
        },
        {
            "name": "oriceon",
            "email": "oriceon@gmail.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "lusitanian/oauth",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/nickryall/PHPoAuthLib",
                    "type": "git",
                    "reference": "dev-master"
                },
                "autoload": {
                    "classmap": [""]
                }
            }
        }
    ],
    "require": {
        "php": ">=5.4",
        "lusitanian/oauth": "dev-master"
    },
    "require-dev": {
        "illuminate/support": "~5"
    },    
    "require-all": true,
    "autoload": {    
        "psr-0": {
            "Artdarek\\OAuth": "src/"
        }
    },    
    "minimum-stability": "dev"
}
